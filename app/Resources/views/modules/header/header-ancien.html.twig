        var navigatorName;
        navigatorName = navigator.appName.toLowerCase();

        if (navigatorName.indexOf("internet explorer") != -1) {
            $("#register_link-connected").hover(function(){
                $("#pub_300_250").css("visibility","hidden");
            },function(){
                $("#pub_300_250").css("visibility","visible");
            });
        }
    });

