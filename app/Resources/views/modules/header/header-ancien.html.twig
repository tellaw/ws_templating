<script>
    jQuery(document).ready(function($){
        //MENU DEROULANT gauche
        $('#menu-cache').on("click",function()
        {
            $('#poussemenu').toggleClass("pousse");
            $('#masthead').toggleClass("pousse");
            $('#content').toggleClass("pousse");
            $('.wrapper>.container').toggleClass("pousse");
            return false;
        });

        $("#poussemenu .arrow-left").click( function(){
            $("#poussemenu .menu-general").show();
            $("#poussemenu .menu-actualite").hide();
        });
        //HOVER BOITE DE CONNEXION
        function initLoginBoxHeader(){
            var $connectButton = $('#register_link-connected');

            var isOpen = false;

            var clickHandler = "click";
            if('ontouchstart' in document.documentElement){
                clickHandler = "touchstart";
            }

            $connectButton.on(clickHandler, function(e){
                if(
                        $(e.target).hasClass('icon-silhouette') ||
                        $(e.target).hasClass('buton-blue') ||
                        $(e.target).attr('id') == "register_link-connected" ||
                        $(e.target).hasClass("sub-content") ){

                    if (!isOpen) {
                        isOpen = true;
                        $connectButton.addClass('open');
                    } else {
                        isOpen = false;
                        $connectButton.removeClass('open');
                    }

                }
            });
        }
        initLoginBoxHeader();

        function isIE () {
            var myNav = navigator.userAgent.toLowerCase();
            return (myNav.indexOf('msie') != -1) ? parseInt(myNav.split('msie')[1]) : false;
        }

        if (isIE () && isIE () < 10) {
            $(".no-connect-header a.buton-blue").attr("href", "/login.html");
            $(".no-connect-header #myti-short-list").remove();
        }

        if (isIE () && isIE () < 10) {
            $(".no-connect-header a.buton-blue").attr("href", "/login.html");
            $(".no-connect-header #myti-short-list").remove();
        }
    });
</script>
